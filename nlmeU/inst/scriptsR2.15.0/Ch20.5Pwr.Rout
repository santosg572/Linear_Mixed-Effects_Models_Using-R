> 
> dtL <- 
+    list(time = c(4, 12, 24, 52),
+         subject = subject)
> dtLong <- expand.grid(dtL)                 # "Long" format
> mrgDt  <- merge(dtLong, dts, sort = FALSE) # Merged
> exmpDt <- 
+    within(mrgDt, 
+           {    
+            m0 <- 65 - 0.1 * time    # Under H0  
+            mA <- 75 - 0.1 * time    # Under Ha. 85 changed to 75
+            mA <- ifelse(treat.f %in% "Active", mA, m0) 
+           })
